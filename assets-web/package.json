{
  "name": "assets-web",
  "version": "0.15.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:prod": "npm run build:prod && npm run server",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "extract": "ngx-translate-extract --input ./projects --output ./projects/assets-web/src/assets/i18n/{ru,en,cn}.json --clean --sort --format namespaced-json --marker marker",
    "lint": "eslint -c .eslintrc.js --ext .ts ./projects/assets-web/src",
    "test": "npm run lint && ng test --configuration=test",
    "watch": "ng test --configuration=test --browsers ChromeHeadless --watch --reporters dots",
    "e2e": "ng e2e",
    "ci": "npm run format:test && ng lint && ng test --configuration=test --browsers ChromeTravisCi --code-coverage && npm run e2e && npm run build:prod -- --deploy-url /assets-web/ --base-href /assets-web",
    "format:write": "prettier projects/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier projects/**/*.{ts,json,md,scss} --list-different",
    "release": "standard-version && git push --follow-tags origin master",
    "analyze": "npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/assets-web/stats-es2015.json",
    "server": "node ./projects/server/server.js",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate && node .all-contributors-html.js",
    "contributors:check": "all-contributors check"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.2.12",
    "@angular/cdk": "16.2.14",
    "@angular/common": "16.2.12",
    "@angular/compiler": "16.2.12",
    "@angular/core": "16.2.12",
    "@angular/forms": "16.2.12",
    "@angular/material": "16.2.14",
    "@angular/platform-browser": "16.2.12",
    "@angular/platform-browser-dynamic": "16.2.12",
    "@angular/router": "16.2.12",
    "@angular/service-worker": "16.2.12",
    "@biesbjerg/ngx-translate-extract-marker": "1.0.0",
    "@fontsource/roboto": "4.5.8",
    "@ngrx/effects": "16.3.0",
    "@ngrx/entity": "16.3.0",
    "@ngrx/router-store": "16.3.0",
    "@ngrx/store": "16.3.0",
    "@ngrx/store-devtools": "16.3.0",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "4.0.0",
    "@types/d3": "5.16.4",
    "bootstrap": "4.6.2",
    "browser-detect": "0.2.28",
    "d3": "5.16.0",
    "date-fns": "2.30.0",
    "hammerjs": "2.0.8",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21",
    "rxjs": "6.5.4",
    "tslib": "2.5.0",
    "uuid": "3.4.0",
    "xlsx": "0.17.5",
    "zone.js": "0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.14",
    "@angular-eslint/eslint-plugin": "^15.1.0",
    "@angular/cli": "^16.2.14",
    "@angular/compiler-cli": "^16.2.12",
    "@angular/language-service": "^16.2.12",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/lodash": "^4.14.191",
    "@types/node": "^12.20.55",
    "@types/uuid": "^3.4.10",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vendure/ngx-translate-extract": "^8.2.1",
    "all-contributors-cli": "^6.24.0",
    "codelyzer": "^6.0.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "express": "^4.18.2",
    "jasmine-core": "4.x",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "^4.0.2",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-spec-reporter": "^0.0.32",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.2",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "standard-version": "^7.1.0",
    "ts-node": "~8.6.2",
    "typescript": "~4.9.5",
    "webpack-bundle-analyzer": "^3.9.0"
  }
}